QPushButton{
    position: relative;
    display: inline-block;
    line-height: 40px;
    width: 100%;
    outline: none;
    color: black;
    text-decoration: none;
    font-size: 20px;
    margin: 0 0;
    border-radius: 6px;
    cursor: pointer;
    height: 40px;
    overflow: hidden;
    background-color: white;
}
QPushButton:hover{
    background-color: #00b0ff40;
}
